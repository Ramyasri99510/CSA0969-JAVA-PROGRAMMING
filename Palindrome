public class palindrom{
    public static void main(String[] args) {
        int n=143;
        int rev=0;
        while(n>0){
            int r=n%10;
            rev=rev*10+r;
            n=n/10;
        }
        if(n==rev){
            System.out.println("yes");
        }
        else{
            System.out.println("no");
        }
    }
}
