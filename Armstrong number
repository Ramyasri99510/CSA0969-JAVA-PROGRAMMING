public class armstrong{
    public static void main(String[] args) {
        int n=153;
        int m=n;
        int sum=0;
        while(n!=0){
            int r=n%10;
            sum=sum+r*r*r;
            n=n/10;
        }
        if(sum==m){
            System.out.println("yes");
        }
        else{
            System.out.println("no");
        }
    }
}
